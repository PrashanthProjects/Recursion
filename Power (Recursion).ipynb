   "source": [
    "def power(base, exp):\n",
    "    assert exp>=0 and int(exp)== exp, \"The Exponent must be positive integer\"\n",
    "    if exp==0:\n",
    "        return 1\n",
    "    if exp==1:\n",
    "        return base\n",
    "    return base*power(base, exp-1)"
   ]
  },
